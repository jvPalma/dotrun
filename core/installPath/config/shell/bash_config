#!/usr/bin/env bash
# DotRun Configuration - Bash Integration
# This file is auto-generated. Do not edit manually.

# Source main config file
if [[ -f "$DR_CONFIG/config/.dotrun_config" ]]; then
  source "$DR_CONFIG/config/.dotrun_config"
fi

# Source category-specific config files
if [[ -d "$DR_CONFIG/config/.dotrun_config.d" ]]; then
  for config_file in "$DR_CONFIG/config/.dotrun_config.d"/*.config; do
    [[ -f "$config_file" ]] && source "$config_file"
  done
fi
